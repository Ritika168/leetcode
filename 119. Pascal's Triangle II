class Solution {
public:
    vector<int> getRow(int rowIndex) {
      long long ans = 1;
      vector<int> ansRow;
      
      for(int col = 0 ; col <= rowIndex ; col++){
         ansRow.push_back(ans);
         ans = ans * (rowIndex - col) / (col + 1);
       
      }   
      return ansRow;
    }
};
